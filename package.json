{
  "name": "clockwork-event-sourcing",
  "version": "0.1.11",
  "description": "Minimal NodeJS(TypeScript) + Redis Event Sourcing Framework",
  "main": "built/index.js",
  "typings": "built/index.d.ts",
  "author": "Kyle Jones <kyle@canadiannomad.com>",
  "homepage": "https://github.com/canadiannomad/clockwork-event-sourcing/issues",
  "url": "https://github.com/canadiannomad/clockwork-event-sourcing",
  "email": "kyle@canadiannomad.com",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest --runInBand",
    "lint": "eslint --fix src/**/*.ts",
    "build": "time tsc --build --pretty --diagnostics --verbose"
  },
  "dependencies": {
    "aws-sdk": "^2.939.0",
    "ioredis": "^4.27.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/ioredis": "^4.26.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.14.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "jest-runtime": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "directories": {
    "doc": "docs",
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canadiannomad/clockwork-event-sourcing.git"
  },
  "bugs": {
    "url": "https://github.com/canadiannomad/clockwork-event-sourcing/issues"
  },
  "files": [
    "built"
  ],
  "jest": {
    "preset": "ts-jest",
    "forceExit": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/test/**/*.test.[jt]s?(x)",
      "**/src/**/?(*.)+test.[jt]s?(x)"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "isolatedModules": true,
        "diagnostics": true
      }
    }
  }
}
